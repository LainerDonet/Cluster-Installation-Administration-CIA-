
default install
prompt 1
timeout 10
display display.msg

label localboot
        LOCALBOOT 0

label install

        KERNEL /boot/x86_64/vmlinuz
        APPEND initrd=/boot/x86_64/initrd.img ip=172.17.253.250 netmask=255.255.0.0 gateway=172.17.100.100 dns=8.8.8.8 BOOTIF=00:25:90:f9:4d:c8 inst.sshd inst.repo=http://172.17.100.100/cia-repo inst.ks=http://172.17.100.100/cia-kickstart/test.ks selinux=0 biosdevname biosdevname=0 net.ifnames=0 selinux=0 SI 
        IPAPPEND 2
